{
  "name": "apollo-transmission",
  "private": true,
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "elasticsearch-sync": "babel-node src/server/scripts/elasticsearchSync.js",
    "migrate:sql": "knex migrate:latest --knexfile ./src/server/knexfile.js",
    "migrate": "npm run migrate:sql && npm run elasticsearch-sync",
    "prettier": "prettier --write \"**/*.js\"",
    "seed": "babel-node src/server/scripts/generateRandomData.js && npm run elasticsearch-sync",
    "start:client": "cross-env PORT=3050 BROWSER=none react-scripts start",
    "start:proxy": "nodemon --watch src/devproxy src/devproxy/index.js",
    "start:server": "nodemon --watch src/server --watch src/shared/utils --watch src/shared/validation --exec babel-node src/server/index.js",
    "start": "concurrently -p \"[{name}]\" -n \"CLIENT,PROXY,SERVER\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" \"npm run start:client\" \"npm run start:proxy\" \"npm run start:server\" ",
    "test:client": "react-scripts test --env=jsdom"
  },
  "author": "",
  "license": "ISC",
